{
  "expo": {
    "name": "Hayotush",
    "slug": "hayotush",
    "version": "1.0.1",
    "sdkVersion": "54.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/android-chrome-512x512.png",
    "scheme": "hayotush",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "linking": {
      "prefixes": ["hayotush://", "https://hayotush.com"]
    },
    "plugins": [
      "expo-router",
      "expo-web-browser",
      "expo-localization",
      "expo-font",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/notification-icon.png",
          "color": "#4CAF50",
          "androidMode": "default",
          "androidCollapsedTitle": "התראות חדשות"
        }
      ],
      [
        "react-native-maps",
        {
          "googleMapsApiKey": "YOUR_GOOGLE_MAPS_API_KEY"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "EXPO_PUBLIC_GOOGLE_EXPO_CLIENT_ID": "387230820014-mc1s8vkumvl98m3e5s82qlevuhfetk3d.apps.googleusercontent.com",
      "EXPO_PUBLIC_GOOGLE_ANDROID_CLIENT_ID": "387230820014-mc1s8vkumvl98m3e5s82qlevuhfetk3d.apps.googleusercontent.com",
      "EXPO_PUBLIC_GOOGLE_IOS_CLIENT_ID": "387230820014-mc1s8vkumvl98m3e5s82qlevuhfetk3d.apps.googleusercontent.com",
      "EXPO_PUBLIC_GOOGLE_WEB_CLIENT_ID": "387230820014-mc1s8vkumvl98m3e5s82qlevuhfetk3d.apps.googleusercontent.com",
      "EXPO_PUBLIC_API_URL": "https://api.hayotush.com/api",
      "router": {},
      "eas": {
        "projectId": "cdbae1c1-512e-4e25-814b-5571a946762b"
      }
    },
    "owner": "orhaklay",
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "android": {
      "package": "com.hayotush.app",
      "versionCode": 2,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION",
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "NOTIFICATIONS"
      ],
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "hayotush"
            },
            {
              "scheme": "https",
              "host": "hayotush.com"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "ios": {
      "bundleIdentifier": "com.hayotush.app",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "האפליקציה זקוקה למיקום כדי לעקוב אחר הטיולים שלך",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "האפליקציה זקוקה למיקום כדי לעקוב אחר הטיולים שלך גם ברקע",
        "NSLocationAlwaysUsageDescription": "האפליקציה זקוקה למיקום כדי לעקוב אחר הטיולים שלך גם ברקע"
      }
    }
  }
}
