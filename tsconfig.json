{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "app/pets/[petId]/reminders/index.jsx",
    "app/pets/[petId]/reminders/new.js",
    "app/pets/test-profile.jsx",
    "theme/theme.js"
  ]
}
